{
    "kind": "Service",
    "apiVersion": "v1",
    "metadata": {
        "name": "gpii-service-couchdb"
    },
    "spec": {
        "type": "NodePort",
        "selector": {
            "app": "gpii"
        },
        "ports": [
            {
                "name": "couchdb",
                "protocol": "TCP",
                "port": 5984,
                "targetPort": 5984
            }
        ]
    }
}
