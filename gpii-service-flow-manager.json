{
    "kind": "Service",
    "apiVersion": "v1",
    "metadata": {
        "name": "gpii-service-flow-manager"
    },
    "spec": {
        "type": "NodePort",
        "selector": {
            "app": "gpii"
        },
        "ports": [
            {
                "name": "flow-manager",
                "protocol": "TCP",
                "port": 8082,
                "targetPort": 8082
            }
        ]
    }
}
